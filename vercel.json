{
  "trailingSlash": true,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/:all*(css|js|svg|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, immutable, max-age=31536000"
        }
      ]
    }
  ],
  "rewrites": [{ "source": "/404", "destination": "/404.html" }]
}
