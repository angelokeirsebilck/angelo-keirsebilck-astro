{
  "trailingSlash": true,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/:all*(css|js|svg|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, immutable, max-age=31536000"
        }
      ]
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "/404.html" }
  ]
}
