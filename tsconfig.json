{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@css/*": ["src/css/*"],
      "@schemas/*": ["src/schemas/*"],
      "@utils/*": ["src/utils/*"],
      "@custom-types/*": ["src/custom-types/*"],
      "@email/*": ["src/email/*"]
    },
    "jsx": "preserve"
  }
}
