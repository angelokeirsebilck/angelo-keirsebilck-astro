---
import type { TwicPicType } from "@custom-types/ui";

export interface Props {
  img: TwicPicType;
}

const { img } = Astro.props;
---

<twic-img {...img}></twic-img>

<script>
  import { TwicImg } from "@twicpics/components/webcomponents";

  customElements.define(`twic-img`, TwicImg);
</script>
